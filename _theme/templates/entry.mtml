<$mt:Include module="共通変数" strip_linefeeds="1"$>
<!DOCTYPE html>
<html lang="ja" itemscope itemtype="http://schema.org/WebPage">
<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width,initial-scale=1">
  <title><$mt:EntryTitle encode_html="1"$> - <$mt:BlogName encode_html="1"$></title>
  <meta name="generator" content="<$mt:ProductName version="1"$>">
  <meta name="description" content="<$mt:EntryExcerpt$>">
  <meta name="keywords" content="<$mt:GetVar name="meta_keywords"$><$mt:EntryKeywords$>">
  <link rel="shortcut icon" type="image/x-icon" href="<$mt:BlogURL encode_html="1"$>common/images/favicon.ico">
  <link rel="start" href="<$mt:BlogURL encode_html="1"$>">
  <link rel="alternate" type="application/atom+xml" title="Recent Entries" href="<$mt:Link template="feed_recent"$>">
  <$mt:CanonicalLink$>
  <meta property="og:type" content="article">
  <meta property="og:locale" content="<$mt:BlogLanguage setvar="blog_lang"$><mt:If name="blog_lang" eq="ja">ja_JP<mt:Else><$mt:Var name="blog_lang"$></mt:If>">
  <meta property="og:title" content="<$mt:EntryTitle encode_html="1"$> | <$mt:BlogName encode_html="1"$>">
  <meta property="og:url" content="<$mt:EntryPermalink encode_html="1"$>">
  <meta property="og:description" content="<$mt:EntryExcerpt$>">
  <meta property="og:site_name" content="<$mt:BlogName encode_html="1"$>">
 <meta property="og:image" content="<mt:If tag="CustomFieldValue" identifier="blog_entry_ogp_image"><mt:CustomFieldAsset identifier="blog_entry_ogp_image"><$mt:AssetThumbnailURL width="1200" $></mt:CustomFieldAsset><mt:Else><$mt:EntryThumbnailURL$></mt:If>"><meta property="fb:app_id" content="<$mt:Var name='fb_app_id'$>">
  <meta name="twitter:card" content="summary_large_image">
  <meta name="twitter:site" content="@<$mt:Var name='twitter_via'$>">
  <meta name="twitter:title" content="<$mt:EntryTitle encode_html="1"$> | <$mt:BlogName encode_html="1"$>">
  <meta name="twitter:description" content="<$mt:EntryExcerpt$>">
  <meta name="twitter:image" content="<mt:If tag="CustomFieldValue" identifier="blog_entry_ogp_image"><mt:CustomFieldAsset identifier="blog_entry_ogp_image"><$mt:AssetThumbnailURL width="1200" $></mt:CustomFieldAsset><mt:Else><$mt:EntryThumbnailURL$></mt:If>">
  <!-- Microdata -->
  <meta itemprop="description" content="<$mt:EntryExcerpt$>">
  <link itemprop="url" href="<$mt:EntryPermalink encode_html="1"$>">
  <link itemprop="image" href="<mt:If tag="CustomFieldValue" identifier="blog_entry_ogp_image"><mt:CustomFieldAsset identifier="blog_entry_ogp_image"><$mt:AssetThumbnailURL width="1200"$></mt:CustomFieldAsset><mt:Else><mt:EntryAssets type="image" sort_order="ascend" limit="1"><$mt:AssetThumbnailURL width="1200"$><mt:Else><$mt:BlogURL encode_html="1"$>common/images/noimage_ogp.png</mt:EntryAssets></mt:If>">
  <$mt:Include module="sample-ToC__list"$>
  <$mt:Include module="JSON-LD"$>
  <$mt:Include module="ヘッダースクリプト"$>
</head>
<body>
  <$mt:Include module="ヘッダー"$>
  <!-- content: start -->
  <div id="content" class="clearfix">
    <div id="main-wrapper">
      <main role="main">
        <article id="entry-<$mt:EntryID$>" class="entry">
          <div class="entry-meta">
          <a href="<$mt:ArchiveLink archive_type='Author'$>">
            <div class="entry-author">
              <figure><img src="<mt:If tag="CustomFieldValue" identifier="writer_profilephoto"><mt:CustomFieldAsset identifier="writer_profilephoto"><$mt:AssetURL$></mt:CustomFieldAsset><mt:elseIf tag='AuthorUserpicURL' square="1"><$mt:AuthorUserpicURL$><mt:Else><$mt:BlogRelativeURL$>common/images/noimage-90.png</mt:If>" width="50" height="auto"></figure>
              <p>
                <mt:If tag="CustomFieldValue" identifier="writer_name"><$mt:CustomFieldValue identifier="writer_name"$><mt:Else><$mt:EntryAuthorDisplayName escape='html'$></mt:If>
              </p>
            </div>
          </a><!-- entry-author -->
          <div class="entry-date">
            <time datetime="<$mt:EntryDate format_name="iso8601"$>"><$mt:EntryDate format="%x %X"$></time>
          </div>
          </div><!-- entry-meta -->

          <h2 class="entry-title"><$mt:EntryTitle encode_html="1"$></h2>

          <section id="entry-categories" class="entry-categories">
            <ul>
            <mt:EntryCategories>
              <li><a href="<$mt:CategoryArchiveLink$>"><$mt:CategoryLabel escape="html"$></a></li>
            </mt:EntryCategories>
            </ul>
          </section><!-- entry-categories -->

          <section id="entry-social-buttons">
            <$mt:Include module="記事ソーシャルボタン"$>
          </section>

          <section class="wysiwyg">
          
            <mt:Ignore>
<mt:BlockEditorBlocks tag="EntryBody">
  <mt:If name="type" eq="custom-tableOfContents">
    <mt:Include module="$__value__" />
  <mt:Else>
    <mt:Var name="__value__" />
  </mt:If>
</mt:BlockEditorBlocks></mt:Ignore>

          <$mt:EntryBody convert_breaks="1"$>
          <$mt:EntryMore convert_breaks="1"$>
          </section><!-- wysiwyg -->

          <section id="entry-social-buttons">
            <$mt:Include module="記事ソーシャルボタン"$>
          </section>
          
          <section class="entry-author-detail">
            <h3>この記事の筆者</h3>
            <div class="entry-author-detail-body clearfix">
              <a href="<$mt:ArchiveLink archive_type='Author'$>">
                <figure>
                  <img src="<mt:If tag="CustomFieldValue" identifier="writer_profilephoto"><mt:CustomFieldAsset identifier="writer_profilephoto"><$mt:AssetURL$></mt:CustomFieldAsset><mt:elseIf tag='AuthorUserpicURL' square="1"><$mt:AuthorUserpicURL$><mt:Else><$mt:BlogRelativeURL$>common/images/noimage-90.png</mt:If>" width="90" height="auto">
                </figure>
              </a>
              <div class="entry-author-detail-body-text">
                <mt:If tag="CustomFieldValue" identifier="writer_title"><p class="title"><$mt:CustomFieldValue identifier="writer_title"$></p></mt:If>
                <p class="name"><mt:If tag="CustomFieldValue" identifier="writer_name"><$mt:CustomFieldValue identifier="writer_name"$><mt:Else><$mt:EntryAuthorDisplayName escape='html'$></mt:If></p>
                <ul class="entry-author-buttons">
                  <mt:If tag="AuthorURL"><li>
                    <a href="<$mt:AuthorURL$>" target="_blank" class="website"></a>
                  </li></mt:If>
                  <mt:If tag="CustomFieldValue" identifier="writer_twitter"><li class="twitter"><a href="https://twitter.com/<$mt:CustomFieldValue identifier="writer_twitter"$>" target="_blank"><!--twitter--></a></li></mt:If>
                  <mt:If tag="CustomFieldValue" identifier="writer_facebook"><li class="facebook"><a href="https://www.facebook.com/<$mt:CustomFieldValue identifier="writer_facebook"$>" target="_blank"><!--facebook--></a></li></mt:If>
                  <mt:If tag="CustomFieldValue" identifier="writer_instagram"><li class="instagram"><a href="https://www.instagram.com/<$mt:CustomFieldValue identifier="writer_instagram"$>" target="_blank"><!--instagram--></a></li></mt:If>
                </ul>
                  
                <mt:If tag="CustomFieldValue" identifier="writer_profile"><p class="profile"><$mt:CustomFieldValue identifier="writer_profile"$></p></mt:If>
                <p class="authorlist">
                  <a href="<$mt:ArchiveLink archive_type='Author'$>">この筆者の記事一覧</a>
                </p>
              </div>
            </div>
         </section>
         <mt:EntryPrimaryCategory>
         <mt:If tag="CategoryCount" ge="2">
         <mt:Ignore>exclude current entry</mt:Ignore>
         <$mt:EntryID setvar="id"$>
         <mt:Entries limit="6">
         <mt:EntriesHeader>
          <section class="relation-entrylist">
            <h3>関連記事</h3>
                <ul>
         </mt:EntriesHeader>
                    <mt:If tag="entryid" ne="$id">
                    <li>
                      <article>
                        <a href="<$mt:EntryPermalink$>">
                        <p class="label category"><mt:EntryCategories type="primary"><$mt:CategoryLabel escape="html"$></mt:EntryCategories></p>
                        <figure class="entry-image">
                          <div class="inner">
                          <img src="<mt:If tag="CustomFieldValue" identifier="blog_entry_ogp_image"><mt:CustomFieldAsset identifier="blog_entry_ogp_image"><$mt:AssetThumbnailURL$></mt:CustomFieldAsset><mt:Else><mt:EntryAssets type="image" sort_order="ascend" limit="1"><$mt:AssetThumbnailURL$><mt:Else><$mt:BlogRelativeURL$>common/images/noimage_ogp.png</mt:EntryAssets></mt:If>" alt="">
                        </div>
                        </figure>
                        <div class="entry-detail">
                            <p class="entry-date">
                              <time datetime="<$mt:EntryDate format_name='iso8601'$>"><$mt:EntryDate format="%x"$></time>
                            </p>
                            <h3 class="entry-title"><$mt:EntryTitle escape="html"$></h3>
                        </div>
                        </a>
                      </article>
                    </li>
                    </mt:If>
         <mt:EntriesFooter>
                  </ul>
            </section><!-- relation-entrylist -->
         </mt:EntriesFooter>
         </mt:Entries>
         </mt:If>
         </mt:EntryPrimaryCategory>

          <section class="entry-navi">
            <div class="entry-navi-prev">
              <mt:EntryPrevious>
                <a href="<$mt:EntryPermalink$>">
                  <p class="header"><i class="fas fa-chevron-left"></i>前の記事へ</p>
                  <p class="title"><$mt:EntryTitle escape="html"$></p>
                </a>
              </mt:EntryPrevious>
            </div>
            <div class="entry-navi-next">
              <mt:EntryNext>
                <a href="<$mt:EntryPermalink$>">
                  <p class="header">次の記事へ<i class="fas fa-angle-right"></i></p>
                  <p class="title"><$mt:EntryTitle escape="html"$></p>
                </a>
              </mt:EntryNext>
            </div>
          </section><!-- entry-navi -->
        </article>
      </main>
    </div>

    <aside>
      <$mt:Include module="サイドバー"$>
    </aside>
  </div>
  <!-- content: end -->
  <$mt:Include module="フッター"$>
</body>
</html>
